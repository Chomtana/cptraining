{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "C++ Launch (GDB)",
			"type": "cppdbg",
			"request": "launch",
			"launchOptionType": "Local",
			"miDebuggerPath": "G:\\MinGW\\bin\\gdb.exe",
			"targetArchitecture": "x64",
			"program": "${file}",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceRoot}",
			"environment": []
		},
		{
			"name": "Python",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"program": "${file}",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "Python Console App",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"program": "${file}",
			"externalConsole": true,
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "Python file",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"program": "${file}",
			"args": ["<","input.in"],
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "Python file out",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"program": "${file}",
			"args": ["<","input.in",">","output.out"],
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
		{
			"name": "Django",
			"type": "python",
			"request": "launch",
			"stopOnEntry": true,
			"program": "${workspaceRoot}/manage.py",
			"args": [
				"runserver",
				"--noreload"
			],
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput",
				"DjangoDebugging"
			]
		}
	]
}